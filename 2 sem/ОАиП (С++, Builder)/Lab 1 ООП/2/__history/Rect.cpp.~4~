//---------------------------------------------------------------------------

#pragma hdrstop

#include "Rect.h"

Rectan :: Rectan(int a, int b, int c, int d)
	{
		x1 = a;
		y1 = b;
		x2 = c;
		y2 = d;
	}

	void Rectan :: show(TCanvas *Canvas)
	{
		Canvas->Pen->Color=clRed;
		Canvas->Pen->Width=3;
		Canvas->Rectangle(x1,y1,x2,y2);
	}

    double Rectan :: Area(int, int)
	{

	}

#pragma package(smart_init)
